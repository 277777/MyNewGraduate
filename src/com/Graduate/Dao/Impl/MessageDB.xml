<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="MessageDB">
	
	    <insert id="AddMessage" parameterType="com.Graduate.Po.Message" >
	        insert into message(userid,msgtext,msgtime) values(#{userid},#{msgtext},#{msgtime});
	    </insert>
	    
	</mapper>
